  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT *,SUM(Importe) Saldo FROM (SELECT
r.id CbteId,r.`Id` CbteReferencia, r.`ProveedorId`,r.`RazonSocial`,r.`Domicilio`,`Proveedor`.`telefono`,
LEFT(CAST(DATE_FORMAT(r.`CbteFecha`, '%d/%m/%Y') as char), 10) CbteFecha,DATE(CAST(r.`CbteFecha` AS DATE)) CbteFecha2,
LEFT(CAST(DATE_FORMAT(r.`FechaVtoPago`, '%d/%m/%Y') as char), 10) CbteVto,`proveedor`.`codigo` CodProv,
r.`CbteTipo`,r.`Denominacion`,r.`CbtePtoVta`,r.`CbteNumero`,r.`CbtePtoVta` RefPtoVta,r.`CbteNumero` RefNumero,
IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) ImporteOriginal,
IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) Importe, r.`Tipo`
FROM `cpracabecera` r,`tipocomprobante` t, Proveedor
WHERE r.`CbteTipo` = t.`Codigo`
AND r.`ProveedorId` = Proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN r.`CbteTipo` IN (1,6,81,82,11,51,83,991)
	WHEN 'P' THEN r.`CbteTipo` IN (991)
	ELSE r.`CbteTipo` IN (1,6,81,82,11,51,83)
	END

AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta

UNION ALL

SELECT
r.id CbteId,c.`CbteReferencia` ,r.`ProveedorId`,r.`RazonSocial`,r.`Domicilio`,`Proveedor`.`telefono`,
LEFT(CAST(DATE_FORMAT(r.`CbteFecha`, '%d/%m/%Y') as char), 10) CbteFecha,DATE(CAST(r.`CbteFecha` AS DATE)) CbteFecha2,
LEFT(CAST(DATE_FORMAT(r.`FechaVtoPago`, '%d/%m/%Y') as char), 10) CbteVto,`proveedor`.`codigo` CodProv,
r.`CbteTipo`,r.`Denominacion`,r.`CbtePtoVta`,r.`CbteNumero`,c.`PtoVta` RefPtoVta,c.`Numero` RefNumero,
IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) ImporteOriginal,
IF(r.`Tipo`='C',c.`Importe`*-1,c.`Importe`) Importe,r.`Tipo`
FROM `cpracabecera` r,`cpraasociado` c,`tipocomprobante` t, Proveedor
WHERE r.id = c.`CbteId`
AND r.`CbteTipo` = t.`Codigo`
AND r.`ProveedorId` = Proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN r.`CbteTipo` NOT IN (1,6,81,82,11,51,83,991)
	WHEN 'P' THEN r.`CbteTipo` IN (991,996)
	ELSE r.`CbteTipo` NOT IN (1,6,81,82,11,51,83,996)
	END

AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta

UNION ALL

SELECT
r.id CbteId,r.`Id` CbteReferencia, r.`proveedorId`,r.`RazonSocial`,r.`Domicilio`,`proveedor`.`telefono`,
LEFT(CAST(DATE_FORMAT(r.`CbteFecha`, '%d/%m/%Y') as char), 10) CbteFecha,DATE(CAST(r.`CbteFecha` AS DATE)) CbteFecha2,
LEFT(CAST(DATE_FORMAT(r.`FechaVtoPago`, '%d/%m/%Y') as char), 10) CbteVto,`proveedor`.`codigo` CodProv,
r.`CbteTipo`,r.`Denominacion`,r.`CbtePtoVta`,r.`CbteNumero`,r.`CbtePtoVta` RefPtoVta,r.`CbteNumero` RefNumero,
r.`ImporteTotal` ImporteOriginal,
(SUM(IFNULL(IF(a.`Tipo`='C',c.`Importe`*-1,c.`Importe`),0))-r.`ImporteTotal`) as Importe, r.`Tipo`
FROM `cpracabecera` r
LEFT JOIN `cpraasociado` c ON r.id = c.`CbteId`
LEFT JOIN `cpracabecera` a ON c.`CbteReferencia` = a.`Id`,
`tipocomprobante` t, proveedor
WHERE r.`CbteTipo` = t.`Codigo`
AND r.`proveedorId` = proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN r.`CbteTipo` IN (996,994)
	WHEN 'P' THEN r.`CbteTipo` IN (996)
	ELSE r.`CbteTipo`  IN (994)
	END

AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta

GROUP BY r.proveedorid,r.id HAVING Importe&lt;&gt;0
ORDER BY proveedorid,CbteTipo,CbteFecha2,CbtePtoVta,CbteNumero

) Pendientes
GROUP BY CbteReferencia HAVING (ABS(Saldo) &gt; 0 AND CbteFecha2 &lt;=@fhasta)
ORDER BY ProveedorId,CbteTipo,CbteFecha2,CbtePtoVta,CbteNumero</CommandText>
