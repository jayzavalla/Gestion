  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT EdoCta.*,IFNULL((Saldo.SaldoAnterior),0)  SaldoAnterior FROM (
SELECT
r.id CbteId,r.`Id` CbteReferencia, r.`ProveedorId`,r.`RazonSocial`,r.`Domicilio`,`Proveedor`.`codigo` codProveedor,`Proveedor`.`telefono`,
LEFT(CAST(DATE_FORMAT(r.`CbteFecha`, '%d/%m/%Y') as char), 10) CbteFecha,CAST(r.`FechaVtoPago` AS DATE) CbteVto,
r.`CbteTipo`,r.`Denominacion`,r.`CbtePtoVta`,r.`CbteNumero`,r.`CbtePtoVta` RefPtoVta,r.`CbteNumero` RefNumero,
IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) ImporteOriginal,
IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) Importe, r.`Tipo`,r.letra
FROM `cpracabecera` r,`tipocomprobante` t, Proveedor
WHERE r.`CbteTipo` = t.`Codigo`
AND r.`ProveedorId` = Proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN r.`CbteTipo` IN (1,6,81,82,11,51,83,111,991)
	WHEN 'P' THEN r.`CbteTipo` IN (991)
	ELSE r.`CbteTipo` IN (1,6,81,82,11,51,83,111)
	END

AND r.`CbteFecha` &gt;=@fdesde
AND r.`CbteFecha` &lt;=@fhasta

AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta

UNION ALL

SELECT
r.id CbteId,c.`CbteReferencia` ,r.`ProveedorId`,r.`RazonSocial`,r.`Domicilio`,`Proveedor`.`codigo` codProveedor,`Proveedor`.`telefono`,
LEFT(CAST(DATE_FORMAT(r.`CbteFecha`, '%d/%m/%Y') as char), 10) CbteFecha,CAST(r.`FechaVtoPago` AS DATE) CbteVto,
r.`CbteTipo`,r.`Denominacion`,r.`CbtePtoVta`,r.`CbteNumero`,c.`PtoVta` RefPtoVta,c.`Numero` RefNumero,
IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) ImporteOriginal,
IF(r.`Tipo`='C',c.`Importe`*-1,c.`Importe`) Importe,r.`Tipo` ,r.letra
FROM `cpracabecera` r,`cpraasociado` c,`tipocomprobante` t, Proveedor
WHERE r.id = c.`CbteId`
AND r.`CbteTipo` = t.`Codigo`
AND r.`ProveedorId` = Proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN r.`CbteTipo` NOT IN (1,6,81,82,11,51,83,111,991)
	WHEN 'P' THEN r.`CbteTipo` IN (991,996)
	ELSE r.`CbteTipo` NOT IN (1,6,81,82,11,51,83,111,996)
	END

AND r.`CbteFecha` &gt;=@fdesde
AND r.`CbteFecha` &lt;=@fhasta

AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta

UNION ALL

SELECT
r.id CbteId,r.`Id` CbteReferencia, r.`proveedorId`,r.`RazonSocial`,r.`Domicilio`,`Proveedor`.`codigo` codProveedor,`proveedor`.`telefono`,
LEFT(CAST(DATE_FORMAT(r.`CbteFecha`, '%d/%m/%Y') as char), 10) CbteFecha,CAST(r.`FechaVtoPago` AS DATE) CbteVto,
r.`CbteTipo`,r.`Denominacion`,r.`CbtePtoVta`,r.`CbteNumero`,0 RefPtoVta,0 RefNumero,
IF(a.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`) ImporteOriginal,
(SUM(IFNULL(IF(a.`Tipo`='C',c.`Importe`*-1,c.`Importe`),0))-r.`ImporteTotal`) Importe, r.`Tipo`,r.letra
FROM `cpracabecera` r
LEFT JOIN `cpraasociado` c ON r.id = c.`CbteId`
LEFT JOIN `cpracabecera` a ON c.`CbteReferencia` = a.`Id`,
`tipocomprobante` t, proveedor
WHERE r.`CbteTipo` = t.`Codigo`
AND r.`proveedorId` = proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN r.`CbteTipo` IN (996,994)
	WHEN 'P' THEN r.`CbteTipo` IN (996)
	ELSE r.`CbteTipo`  IN (994)
	END
AND r.`CbteFecha` &gt;=@fdesde
AND r.`CbteFecha` &lt;=@fhasta

AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta
GROUP BY r.proveedorid,r.id HAVING Importe&lt;&gt;0
ORDER BY proveedorid,CbteFecha,CbtePtoVta,CbteNumero

) as EdoCta

LEFT JOIN
(

SELECT r.ProveedorId ProvId,SUM(IF(r.`Tipo`='C',r.`ImporteTotal`*-1,r.`ImporteTotal`)) SaldoAnterior
FROM `cpracabecera` r,`tipocomprobante` t, Proveedor
WHERE r.`CbteTipo` = t.`Codigo`
AND r.`ProveedorId` = Proveedor.`id`
AND CASE @tipo
	WHEN 'T' THEN 0=0
	WHEN 'P' THEN r.`CbteTipo` IN (991,992,995,996)
	ELSE r.`CbteTipo` NOT IN (991,992,995,996)
	END

AND r.`CbteFecha` &lt;@fdesde
AND r.ProveedorId &gt;= @cdesde AND r.ProveedorId &lt;= @chasta
GROUP BY r.ProveedorId) Saldo
ON EdoCta.ProveedorId =Saldo.ProvId

ORDER BY ProveedorId,CbteFecha,CbteId,CbtePtoVta,CbteNumero</CommandText>
        <QueryParameters>
